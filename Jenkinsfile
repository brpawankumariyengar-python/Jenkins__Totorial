// Declarative //
pipeline{
    agent {label 'Computer'}
    stages{

        stage('Multple__Bash__Script__Lines'){
                steps{
                    sh """
                        pwd
                        whoami
                        ls -a
                        printenv
                        """
                    }
        }
        stage('Completeion__Celebration'){
            steps{
                echo "Yaaaay ...Done Done Done... Ho gaya"
                }
        }
        stage('hohohoho'){
            steps{
                echo 'ho ho ho ho ho ho ho'
            }
        }
    }
}
