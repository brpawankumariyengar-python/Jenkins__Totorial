pipeline {
    agent any
    stages {
        stage("Barfi"){
            steps {
                writeFile(file: "filename.txt", text: "Hule Hulare Hule Hule Hule")
            }
        }
        stage("Jalebi"){
            steps {
                sh "mkdir momomomo"
            }
        }
        stage("Pakoda"){
            steps {
                    echo "Hona tha  .... Ho Gaya"
                   }
            }
        stage("Khalaas"){
            steps {
                    sh "pwd"
                   }
            }
    }
}
